#include <bits/stdc++.h>
#include <iostream>
#define ll long long
using namespace std;
int n;
ll k;
int needed[3];
bool can (int have[], int needed[], int price[], ll mid){
    ll money = k;
    for (int i = 0; i < 3; i++){
            ll required = needed[i] * mid;
        //if (required < have[i]){
        //    continue;
        //}
        if (required > have[i]){
            money -= (required - have[i]) * price[i];
        }
    }
    if (money >= 0) {
        return true;
    }
    else {
        return false;
    }
}

int main()
{
ios::sync_with_stdio(0); cin.tie(0);

string s;
cin >> s;
for (int i = 0; i < s.size(); i++){
    if (s[i] == 'B'){
        needed[0]++;
    }
    else if (s[i] == 'S'){
        needed[1]++;
    }
    else if (s[i] == 'C'){
        needed[2]++;
    }
}
int have[3];
int price[3];
for (int i = 0; i < 3; i++){
    cin >> have[i];
}
for (int i = 0; i < 3; i++){
    cin >> price[i];
}
cin >> k;
ll l = 0, r = 1e15;
ll mid;
ll ans = 0;
while (l <= r){
    mid = (l + r) / 2;
    if (can(have, needed, price, mid)){
        ans = mid;
        l = mid + 1;
    }
    else {
        r = mid - 1;
    }
}
cout << ans << endl;

    return 0;

}
